<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
	  <Version>2.0.0</Version>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <NoWarn>1701;1702;CS1591</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <Authors>Sewer56</Authors>
	  <Company />
	  <Description>Contains code for communicating remotely with Reloaded-II's Mod Loader when loaded inside a remote process. Allows for querying information as well as load/unload/resume/suspend of mods in real time. This is an "internal" package; no documentation is provided and the API can change at any time.</Description>
	  <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
	  <PackageProjectUrl>https://github.com/Reloaded-Project/Reloaded-II</PackageProjectUrl>
	  <PackageIconUrl>https://avatars1.githubusercontent.com/u/45473408</PackageIconUrl>
	  <RepositoryUrl>https://github.com/Reloaded-Project/Reloaded-II</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Reloaded.Messaging" Version="1.3.0" />
    <PackageReference Include="Reloaded.Messaging.Serializer.MessagePack" Version="1.1.0" />
  </ItemGroup>
	
  <ItemGroup>
	<None Include="..\..\LICENSE.md">
	  <Pack>True</Pack>
	  <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
